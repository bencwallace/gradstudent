{
    "files.associations": {
      "h": "h",
      "cpp": "cpp"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.o": true
    },
    "editor.indentSize": 2,
    "editor.detectIndentation": false,
    "C_Cpp.intelliSenseEngine": "disabled"
}